<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.monitor.dao.KaptchaDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.monitor.entity.Kaptcha">
        <id column="id" property="id" />
        <result column="right_code" property="rightCode" />
        <result column="address_ip" property="addressIp" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, right_code AS rightCode, address_ip AS addressIp, create_time AS createTime
    </sql>

</mapper>
